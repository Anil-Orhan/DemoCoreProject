@model List<Demo.Core.Entities.DTO.EmployeeSalesReport>


    <div class="container mt-3" >
          
        <table style="color:aliceblue" class="table table-hover">
            <thead>
            <tr>
                <th>Employee ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Order Amounth</th>
                <th>Total Sale</th>
            </tr>
            </thead>
            <tbody>
            
                
                @foreach (var item in Model)
                {<tr >


                    <td>@item.EmployeeID</td>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.OrderAmounth</td>
                        <td>@item.TotalSale</td>
                 </tr>
                }
               
           
            </tbody>
        </table>
    </div>

   
   
